  "shortcuts": [
    {
      "name": "New Incident",
      "short_name": "New Incident",
      "description": "Create a new incident report",
      "url": "/incidents?action=new",
      "icons": [
        {
          "src": "/icon.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View event dashboard",
      "url": "/dashboard",
      "icons": [
        {
          "src": "/icon.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Staff Management",
      "short_name": "Staff",
      "description": "Manage staff and assignments",
      "url": "/staff",
      "icons": [
        {
          "src": "/icon.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "View event analytics",
      "url": "/analytics",
      "icons": [
        {
          "src": "/icon.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Settings",
      "short_name": "Settings",
      "description": "Manage app settings",
      "url": "/settings",
      "icons": [
        {
          "src": "/icon.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshot-wide.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide"
    },
    {
      "src": "/screenshot-narrow.png",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow"
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate",
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+incommand",
      "url": "/?action=%s"
    }
  ],
  "iarc_rating_id": "",
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "permissions": [
    "notifications",
    "geolocation"
  ]
} 