    "framer-motion": "^12.19.1",
    "fuse.js": "^7.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.544.0",
    "next": "^14.2.33",
    "openai": "^4.104.0",
    "pdf-lib": "^1.17.1",
    "radix-ui": "^1.4.3",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18",
