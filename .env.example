# Supabase Configuration
# Copy this file to .env.local and fill in your actual values

# Required: Supabase URL (same for both client and server)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
SUPABASE_URL=https://your-project.supabase.co

# Required: Supabase Anonymous Key (for client-side operations)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_ANON_KEY=your_anon_key_here

# Required: Supabase Service Role Key (for server-side operations)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# AI API Keys (for Task Master and other AI features)
ANTHROPIC_API_KEY=your_anthropic_key_here
PERPLEXITY_API_KEY=your_perplexity_key_here
OPENAI_API_KEY=your_openai_key_here
GOOGLE_API_KEY=your_google_key_here
MISTRAL_API_KEY=your_mistral_key_here
XAI_API_KEY=your_xai_key_here
OPENROUTER_API_KEY=your_openrouter_key_here
